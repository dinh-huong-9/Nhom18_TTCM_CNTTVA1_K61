<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tra Sen Bach Diep</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
<link href="~/Content/Site.css" rel="stylesheet" />
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color:white">
        <div class="container" style="height:50px">
            <div class="navbar-header" style="background-color:white">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img class="navbar-brand" src="~/Pictures/trang-chu/logophongtra.png" width="200" height="80" style="padding:0" />
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>
            </div>
        </div>
    </div>

   
    <div class=" body-content">
        @RenderBody()
    </div>
    <div>
        <footer class="footer">
            <img class="img_footer" src="~/Pictures/trang-chu/fotter.png" width="1500" height="200" />
            <div class="row">
                <div class="col-md-4">
                    <img class="navbar-brand" src="~/Pictures/trang-chu/logophongtra.png" width="200" height="80" style="padding:0" />
                    <ul style="padding-top:60px">
                        <li>7999-9999-9999</li>
                        <li>trasenbachdiep@gmail.com</li>
                        <li> Số 12 Quảng An, Tây Hồ,Hà Nội</li>
                    </ul>
                </div>
                <div class="col-md-4">
                    <h2 style="margin-left:40px">Về Chúng Tôi </h2>
                    <ul>
                        <li>Giới thiệu</li>
                        <li>Sản phẩm</li>
                        <li> Tin tức</li>
                    </ul>

                </div>
                <div class="col-md-4">
                    <h2 style="margin-left:40px">Liên Hệ</h2>
                    <ul>
                        <li>Coppyright  2019-2020</li>
                        <li>Tra Sen Bach Diep. All rights reserved</li>

                    </ul>

                </div>
            </div>
        </footer>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
